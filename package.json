{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "tree": "find . -not -path '*/node_modules/*' -not -path '*/dist/*' -not -path '*/.git/*'",
    "server": "node --env-file=.env --import tsx server/api.ts",
    "server:watch": "node --env-file=.env --watch --import tsx server/api.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run server:watch\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "framer-motion": "^12.23.22",
    "i18next": "^25.5.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.7.3",
    "react-router-dom": "^7.8.2",
    "resend": "^6.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.0.6"
  }
}
